# Login access control table, scanned for first entry matching user@origin
# format: permission : users/groups : origins
#
- : ALL : tsunami.ocf.berkeley.edu biohazard.ocf.berkeley.edu
- : (sorry) : cron crond
+ : ALL : cron crond
+ : root : LOCAL hal.ocf.berkeley.edu pandemic.ocf.berkeley.edu jaws.ocf.berkeley.edu supernova.ocf.berkeley.edu eruption.ocf.berkeley.edu mercury.techxonline.net
+ : apt-dater : lightning.ocf.berkeley.edu
+ : nessus : .security.berkeley.edu
<% @ulogin.each do |user,from| -%>
<% if not user.nil? and not from.nil? -%>
+ : <%= user %> : <%= from %>
<% end -%>
<% end -%>
+ : (ocfroot) : ALL
+ : (ocfstaff) : ALL
+ : (sys) : ALL
<% @glogin.each do |group,from| -%>
<% if not group.nil? and not from.nil? -%>
+ : (<%= group %>) : <%= from %>
<% end -%>
<% end -%>
- : ALL : ALL
