<VirtualHost *:80>
    DirectoryIndex docs.html
    DocumentRoot /var/www/
    ServerName docs.ocf.berkeley.edu
    ServerAlias docs
    <Directory /var/www/>
       Options Indexes FollowSymLinks MultiViews
       AllowOverride All
       Order allow,deny
       allow from all
    </Directory>

ErrorLog "/var/log/apache2/wiki-error.log"
</VirtualHost>

<IfModule alias_module>
	Alias /wiki /var/www/w/index.php
        Alias /staffwiki /var/www/sw/index.php
</IfModule>
