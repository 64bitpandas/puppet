auto lo
iface lo inet loopback

auto br0
allow-hotplug br0
iface br0 inet static
<% if octet.to_i > 192 -%>
        address 128.32.129.<%= octet %>
        netmask 255.255.255.192
        gateway 128.32.129.193
<% elsif octet.to_i > 65 && octet.to_i < 127 -%>
        address 169.229.172.<%= octet %>
        netmask 255.255.255.192
        gateway 169.229.172.65
<% end -%>
        bridge_ports eth0
        bridge_stp off
        bridge_maxwait 0
        bridge_fd 0
