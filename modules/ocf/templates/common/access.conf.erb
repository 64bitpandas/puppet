# Login access control table, scanned for first entry matching user@origin
# format: permission : users/groups : origins
#
+ : ALL : cron crond
+ : root : LOCAL hal.ocf.berkeley.edu pandemic.ocf.berkeley.edu
+ : apt-dater : lightning.ocf.berkeley.edu
+ : dumper : pollution.ocf.berkeley.edu
+ : nessus : .security.berkeley.edu
<% login_users.each do |user,from| -%>
+ : <%= user %> : <%= from %>
<% end -%>
+ : (admin) : ALL
+ : (ocfstaff) : ALL
+ : (sys) : ALL
<% login.each do |val| -%>
+ : (<%= val %>) : ALL
<% end -%>
- : ALL : ALL
